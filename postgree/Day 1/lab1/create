create Table student(
	id serial primary key,
	e_name varchar(40),
	email varchar(50),
	address varchar(50),
	phone varchar(13) Unique ,
	track_id integer, --forign key
	gender VARCHAR(6) DEFAULT 'male' check (gender IN ('male', 'female'))
);

create Table track(
	id serial primary key,
	track_name varchar(40)
);

create Table subject(
	id serial primary key,
	sub_name varchar(40),
	max_score numeric
	
);

create Table exam(
	id serial primary key,
	date date
);

create Table grades(
-- comp pk
	stu_id integer ,--forign key ckp1
	sub_id integer ,--forign key cpk2
	exam_id integer,--forign key
	grade numeric
);

create Table track_sub(
	track_id integer ,--forign key ckp1
	sub_id integer	--forign key cpk2
);

create Table stu_sub(
	stu_id integer ,--forign key ckp1
	sub_id integer	--forign key cpk2
);
